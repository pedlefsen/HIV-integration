} else {
if( file.exists( SCRIData.countIS.the.samples.Rda.filename ) ) {
    load( file = SCRIData.countIS.the.samples.Rda.filename );
} else {
    # Note that we reuse the samples drawn for analyzing uniqueISs; we just change them so the total count is all ISs, rather than unique ISs.
    SCRIData.countIS.the.samples <- lapply( names( SCRIData.num.countIS.by.subject.and.total.and.totalProliferating ), function ( .the.result ) {
        .num.ISs.for.each.uniqueIS <- SCRIData.num.countIS.by.subject.and.total.and.totalProliferating[[ .the.result ]];
        print( .num.ISs.for.each.uniqueIS );
        .SCRIData.num.integration.sites <- length( .num.ISs.for.each.uniqueIS );
        # For simplicity we draw the # of unique ISs per gene, then we randomly assign the .num.ISs.for.each.uniqueIS to them.
        # But we can use the draws we've already drawn (SCRIData.the.samples).
        .the.samples <-
        apply( SCRIData.the.samples[[ .the.result ]], 1, function( .one.sample.num.uniqueISs.per.gene ) {
            # pre-condition: total count is # unique ISs.
            print( sum( .one.sample.num.uniqueISs.per.gene ));
            stopifnot( sum( .one.sample.num.uniqueISs.per.gene ) == SCRIData.num.uniqueIS.by.subject.and.total.and.totalProliferating[[ .the.result ]] );
            # Randomly shuffle the values of .num.ISs.for.each.uniqueIS and replicate each uniqueIS the corresponding number of times.
            ..SCRIData.num.integration.sites.shuffled <- sample( .num.ISs.for.each.uniqueIS );
            ..SCRIData.num.integration.sites.shuffled.index <- 0;
            .sample.num.ISs.per.gene <-
            sapply( .one.sample.num.uniqueISs.per.gene, function( .num.for.gene ) {
                if( .num.for.gene == 0 ) {
                    return( 0 );
                }
                .rv <- sum( ..SCRIData.num.integration.sites.shuffled[ ..SCRIData.num.integration.sites.shuffled.index + 1:.num.for.gene ] );
                #print( .rv );
                ..SCRIData.num.integration.sites.shuffled.index <<- ..SCRIData.num.integration.sites.shuffled.index + .num.for.gene;
                return( .rv );
            } );
            # post-condition: total count is # all ISs.
            stopifnot( sum( .sample.num.ISs.per.gene ) == SCRIData.num.IS.by.subject.and.total.and.totalProliferating[[ .the.result ]] );
            #print( sum( .sample.num.ISs.per.gene ) );
            return( .sample.num.ISs.per.gene );
        } );
        return( t( .the.samples ) );
    } );
    names( SCRIData.countIS.the.samples ) <- names( SCRIData.num.countIS.by.subject.and.total.and.totalProliferating );
    save( SCRIData.countIS.the.samples, file = SCRIData.countIS.the.samples.Rda.filename );
}

