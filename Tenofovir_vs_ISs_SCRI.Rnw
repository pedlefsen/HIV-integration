<<echo = FALSE, results = hide>>=
if( exists( "SCRI.IS.P.VALUES.ARE.COMPUTED.ALREADY" ) && SCRI.IS.P.VALUES.ARE.COMPUTED.ALREADY ) {
    ## GREAT.
} else {

## Calculate conservative two-sided p-values for the observed number of ISs with each feature exceeding the observed number.
# first, for each subject separately:
SCRIData.IS.p.values.by.feature.by.subject <-
  t( sapply( SCRIData.subjectIDs, function( .subject.id ) {
      sapply( colnames( Wang.feature.by.gene ), function ( .feature ) {
          calculatePValue( SCRIData.num.IS.by.subject.by.feature[ .subject.id, .feature ], SCRIData.IS.feature.nulls[[ .subject.id ]] )
      } )
  } ) );
# Next, for all subjects pooled:
SCRIData.IS.p.values.by.feature <-
  sapply( colnames( Wang.feature.by.gene ), function ( .feature ) {
      calculatePValue( SCRIData.num.IS.by.feature[ .feature ], SCRIData.IS.feature.nulls[[ "total" ]][ , .feature ] )
  } );
# Finally, for all subjects pooled, but just looking at proliferating ISs:
SCRIData.IS.p.values.proliferating.by.feature <-
  sapply( colnames( Wang.feature.by.gene ), function ( .feature ) {
      calculatePValue( SCRIData.num.IS.proliferating.by.feature[ .feature ], SCRIData.IS.feature.nulls[[ "totalProliferating" ]][ , .feature ] )
  } );
# Finally, for all subjects pooled, but just looking at nonproliferating ISs:
SCRIData.IS.p.values.nonproliferating.by.feature <-
  sapply( colnames( Wang.feature.by.gene ), function ( .feature ) {
      calculatePValue( SCRIData.num.IS.nonproliferating.by.feature[ .feature ], SCRIData.IS.feature.nulls[[ "totalNonproliferating" ]][ , .feature ] )
  } );
## Note that we pre-specified that we'd ignore all but the top 20 GO terms, but we lazily did the analysis for all of them.

.isFeatures <- grep( "^is", names( SCRIData.IS.p.values.proliferating.by.feature ), value = T )
primaryFeatures.Tenofovir <- c( "isDownregulatedByTenofovir.exvivo", "isUpregulatedByTenofovir.exvivo", "isDownregulatedByTenofovir.Rectal", "isUpregulatedByTenofovir.Rectal" );
primaryFeatures.GO <- c( "isAnyTop20.GO" );
primaryFeatures.cancer <- c( "isCancer", "isAnyCancer.GO" );
secondaryFeatures.Tenofovir <- setdiff( grep( "Tenofovir", .isFeatures, value = TRUE ), primaryFeatures.Tenofovir );
secondaryFeatures.cancer10 <- setdiff( .isFeatures, c( primaryFeatures.GO, primaryFeatures.cancer, primaryFeatures.Tenofovir, secondaryFeatures.Tenofovir ) );
stopifnot( length( secondaryFeatures.cancer10 ) == 10 );
individualGOFeatures.Hallmark40 <- names( SCRIData.IS.p.values.proliferating.by.feature )[ 1:40 ];
stopifnot( length( individualGOFeatures.Hallmark40 ) == 40 );
individualGOFeatures.top20 <- names( SCRIData.IS.p.values.proliferating.by.feature )[ 53:(53 + 19) ];
stopifnot( length( individualGOFeatures.top20 ) == 20 );

# FWER-correct (using "holm" method) the p-values for the cancer10 and the Hallmark40 and the top20.
SCRIData.IS.p.values.by.feature.by.subject.adjusted <- t( apply( SCRIData.IS.p.values.by.feature.by.subject, 1, function( .p.values.by.feature ) {
    .p.values.by.feature[ primaryFeatures.Tenofovir ] <- p.adjust( .p.values.by.feature[ primaryFeatures.Tenofovir ], method = "holm" );
    .p.values.by.feature[ secondaryFeatures.Tenofovir ] <- p.adjust( .p.values.by.feature[ secondaryFeatures.Tenofovir ], method = "holm" );
    .p.values.by.feature[ secondaryFeatures.cancer10 ] <- p.adjust( .p.values.by.feature[ secondaryFeatures.cancer10 ], method = "holm" );
    .p.values.by.feature[ individualGOFeatures.Hallmark40 ] <- p.adjust( .p.values.by.feature[ individualGOFeatures.Hallmark40 ], method = "holm" );
    .p.values.by.feature[ individualGOFeatures.top20 ] <- p.adjust( .p.values.by.feature[ individualGOFeatures.top20 ], method = "holm" );
    return( .p.values.by.feature );
} ) );
SCRIData.IS.p.values.by.feature.adjusted <- SCRIData.IS.p.values.by.feature;
SCRIData.IS.p.values.by.feature.adjusted[ primaryFeatures.Tenofovir ] <- p.adjust( SCRIData.IS.p.values.by.feature[ primaryFeatures.Tenofovir ], method = "holm" );
SCRIData.IS.p.values.by.feature.adjusted[ secondaryFeatures.Tenofovir ] <- p.adjust( SCRIData.IS.p.values.by.feature[ secondaryFeatures.Tenofovir ], method = "holm" );
SCRIData.IS.p.values.by.feature.adjusted[ secondaryFeatures.cancer10 ] <- p.adjust( SCRIData.IS.p.values.by.feature[ secondaryFeatures.cancer10 ], method = "holm" );
SCRIData.IS.p.values.by.feature.adjusted[ individualGOFeatures.Hallmark40 ] <- p.adjust( SCRIData.IS.p.values.by.feature[ individualGOFeatures.Hallmark40 ], method = "holm" );
SCRIData.IS.p.values.by.feature.adjusted[ individualGOFeatures.top20 ] <- p.adjust( SCRIData.IS.p.values.by.feature[ individualGOFeatures.top20 ], method = "holm" );

## Adjust the proliferating results
SCRIData.IS.p.values.proliferating.by.feature.adjusted <- SCRIData.IS.p.values.proliferating.by.feature;
SCRIData.IS.p.values.proliferating.by.feature.adjusted[ primaryFeatures.Tenofovir ] <- p.adjust( SCRIData.IS.p.values.proliferating.by.feature[ primaryFeatures.Tenofovir ], method = "holm" );
SCRIData.IS.p.values.proliferating.by.feature.adjusted[ secondaryFeatures.Tenofovir ] <- p.adjust( SCRIData.IS.p.values.proliferating.by.feature[ secondaryFeatures.Tenofovir ], method = "holm" );
SCRIData.IS.p.values.proliferating.by.feature.adjusted[ secondaryFeatures.cancer10 ] <- p.adjust( SCRIData.IS.p.values.proliferating.by.feature[ secondaryFeatures.cancer10 ], method = "holm" );
SCRIData.IS.p.values.proliferating.by.feature.adjusted[ individualGOFeatures.Hallmark40 ] <- p.adjust( SCRIData.IS.p.values.proliferating.by.feature[ individualGOFeatures.Hallmark40 ], method = "holm" );
SCRIData.IS.p.values.proliferating.by.feature.adjusted[ individualGOFeatures.top20 ] <- p.adjust( SCRIData.IS.p.values.proliferating.by.feature[ individualGOFeatures.top20 ], method = "holm" );

## Adjust the nonproliferating results
SCRIData.IS.p.values.nonproliferating.by.feature.adjusted <- SCRIData.IS.p.values.nonproliferating.by.feature;
SCRIData.IS.p.values.nonproliferating.by.feature.adjusted[ primaryFeatures.Tenofovir ] <- p.adjust( SCRIData.IS.p.values.nonproliferating.by.feature[ primaryFeatures.Tenofovir ], method = "holm" );
SCRIData.IS.p.values.nonproliferating.by.feature.adjusted[ secondaryFeatures.Tenofovir ] <- p.adjust( SCRIData.IS.p.values.nonproliferating.by.feature[ secondaryFeatures.Tenofovir ], method = "holm" );
SCRIData.IS.p.values.nonproliferating.by.feature.adjusted[ secondaryFeatures.cancer10 ] <- p.adjust( SCRIData.IS.p.values.nonproliferating.by.feature[ secondaryFeatures.cancer10 ], method = "holm" );
SCRIData.IS.p.values.nonproliferating.by.feature.adjusted[ individualGOFeatures.Hallmark40 ] <- p.adjust( SCRIData.IS.p.values.nonproliferating.by.feature[ individualGOFeatures.Hallmark40 ], method = "holm" );
SCRIData.IS.p.values.nonproliferating.by.feature.adjusted[ individualGOFeatures.top20 ] <- p.adjust( SCRIData.IS.p.values.nonproliferating.by.feature[ individualGOFeatures.top20 ], method = "holm" );

SCRI.IS.P.VALUES.ARE.COMPUTED.ALREADY <- TRUE;
} # End if SCRI.IS.P.VALUES.ARE.COMPUTED.ALREADY .. else ..
