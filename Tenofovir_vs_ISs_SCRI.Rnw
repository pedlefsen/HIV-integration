<<echo = FALSE, results = hide>>=
if( exists( "SCRI.uniqueIS.P.VALUES.ARE.COMPUTED.ALREADY" ) && SCRI.uniqueIS.P.VALUES.ARE.COMPUTED.ALREADY ) {
    ## GREAT.
} else {

## Calculate conservative two-sided p-values for the observed number of ISs with each feature exceeding the observed number.
# first, for each subject separately:
SCRIData.p.values.by.feature.by.subject <-
  t( sapply( SCRIData.subjectIDs, function( .subject.id ) {
      sapply( colnames( Wang.feature.by.gene ), function ( .feature ) {
          calculatePValue( SCRIData.num.uniqueIS.by.subject.by.feature[ .subject.id, .feature ], SCRIData.feature.nulls[[ .subject.id ]] )
      } )
  } ) );
# Next, for all subjects pooled:
SCRIData.p.values.by.feature <-
  sapply( colnames( Wang.feature.by.gene ), function ( .feature ) {
      calculatePValue( SCRIData.num.uniqueIS.by.feature[ .feature ], SCRIData.feature.nulls[[ "total" ]][ , .feature ] )
  } );
# Finally, for all subjects pooled, but just looking at proliferating ISs:
SCRIData.p.values.proliferating.by.feature <-
  sapply( colnames( Wang.feature.by.gene ), function ( .feature ) {
      calculatePValue( SCRIData.num.uniqueIS.proliferating.by.feature[ .feature ], SCRIData.feature.nulls[[ "totalProliferating" ]][ , .feature ] )
  } );
# Finally, for all subjects pooled, but just looking at nonproliferating ISs:
SCRIData.p.values.nonproliferating.by.feature <-
  sapply( colnames( Wang.feature.by.gene ), function ( .feature ) {
      calculatePValue( SCRIData.num.uniqueIS.nonproliferating.by.feature[ .feature ], SCRIData.feature.nulls[[ "totalNonproliferating" ]][ , .feature ] )
  } );
## Note that we pre-specified that we'd ignore all but the top 20 GO terms, but we lazily did the analysis for all of them.
.isFeatures <- grep( "^is", names( SCRIData.p.values.proliferating.by.feature ), value = T )
primaryFeatures.Tenofovir <- c( "isDownregulatedByTenofovir.exvivo", "isUpregulatedByTenofovir.exvivo", "isDownregulatedByTenofovir.Rectal", "isUpregulatedByTenofovir.Rectal" );
primaryFeatures.GO <- c( "isAnyTop20.GO" );
primaryFeatures.cancer <- c( "isCancer", "isAnyCancer.GO" );
secondaryFeatures.Tenofovir <- setdiff( grep( "Tenofovir", .isFeatures, value = TRUE ), primaryFeatures.Tenofovir );
secondaryFeatures.cancer10 <- setdiff( .isFeatures, c( primaryFeatures.GO, primaryFeatures.cancer, primaryFeatures.Tenofovir, secondaryFeatures.Tenofovir ) );
stopifnot( length( secondaryFeatures.cancer10 ) == 10 );
individualGOFeatures.Hallmark40 <- names( SCRIData.p.values.proliferating.by.feature )[ 1:40 ];
stopifnot( length( individualGOFeatures.Hallmark40 ) == 40 );
individualGOFeatures.top20 <- names( SCRIData.p.values.proliferating.by.feature )[ 53:(53 + 19) ];
stopifnot( length( individualGOFeatures.top20 ) == 20 );

# FWER-correct (using "holm" method) the p-values for the cancer10 and the Hallmark40 and the top20.
SCRIData.p.values.by.feature.by.subject.adjusted <- t( apply( SCRIData.p.values.by.feature.by.subject, 1, function( .SCRIData.p.values.by.feature ) {
    .SCRIData.p.values.by.feature[ primaryFeatures.Tenofovir ] <- p.adjust( .SCRIData.p.values.by.feature[ primaryFeatures.Tenofovir ], method = "holm" );
    .SCRIData.p.values.by.feature[ secondaryFeatures.Tenofovir ] <- p.adjust( .SCRIData.p.values.by.feature[ secondaryFeatures.Tenofovir ], method = "holm" );
    .SCRIData.p.values.by.feature[ secondaryFeatures.cancer10 ] <- p.adjust( .SCRIData.p.values.by.feature[ secondaryFeatures.cancer10 ], method = "holm" );
    .SCRIData.p.values.by.feature[ individualGOFeatures.Hallmark40 ] <- p.adjust( .SCRIData.p.values.by.feature[ individualGOFeatures.Hallmark40 ], method = "holm" );
    .SCRIData.p.values.by.feature[ individualGOFeatures.top20 ] <- p.adjust( .SCRIData.p.values.by.feature[ individualGOFeatures.top20 ], method = "holm" );
    return( .SCRIData.p.values.by.feature );
} ) );
SCRIData.p.values.by.feature.adjusted <- SCRIData.p.values.by.feature;
SCRIData.p.values.by.feature.adjusted[ primaryFeatures.Tenofovir ] <- p.adjust( SCRIData.p.values.by.feature[ primaryFeatures.Tenofovir ], method = "holm" );
SCRIData.p.values.by.feature.adjusted[ secondaryFeatures.Tenofovir ] <- p.adjust( SCRIData.p.values.by.feature[ secondaryFeatures.Tenofovir ], method = "holm" );
SCRIData.p.values.by.feature.adjusted[ secondaryFeatures.cancer10 ] <- p.adjust( SCRIData.p.values.by.feature[ secondaryFeatures.cancer10 ], method = "holm" );
SCRIData.p.values.by.feature.adjusted[ individualGOFeatures.Hallmark40 ] <- p.adjust( SCRIData.p.values.by.feature[ individualGOFeatures.Hallmark40 ], method = "holm" );
SCRIData.p.values.by.feature.adjusted[ individualGOFeatures.top20 ] <- p.adjust( SCRIData.p.values.by.feature[ individualGOFeatures.top20 ], method = "holm" );

## adjust the proliferating results
SCRIData.p.values.proliferating.by.feature.adjusted <- SCRIData.p.values.proliferating.by.feature;
SCRIData.p.values.proliferating.by.feature.adjusted[ primaryFeatures.Tenofovir ] <- p.adjust( SCRIData.p.values.proliferating.by.feature[ primaryFeatures.Tenofovir ], method = "holm" );
SCRIData.p.values.proliferating.by.feature.adjusted[ secondaryFeatures.Tenofovir ] <- p.adjust( SCRIData.p.values.proliferating.by.feature[ secondaryFeatures.Tenofovir ], method = "holm" );
SCRIData.p.values.proliferating.by.feature.adjusted[ secondaryFeatures.cancer10 ] <- p.adjust( SCRIData.p.values.proliferating.by.feature[ secondaryFeatures.cancer10 ], method = "holm" );
SCRIData.p.values.proliferating.by.feature.adjusted[ individualGOFeatures.Hallmark40 ] <- p.adjust( SCRIData.p.values.proliferating.by.feature[ individualGOFeatures.Hallmark40 ], method = "holm" );
SCRIData.p.values.proliferating.by.feature.adjusted[ individualGOFeatures.top20 ] <- p.adjust( SCRIData.p.values.proliferating.by.feature[ individualGOFeatures.top20 ], method = "holm" );
SCRI.uniqueIS.P.VALUES.ARE.COMPUTED.ALREADY <- TRUE;

## adjust the nonproliferating results
SCRIData.p.values.nonproliferating.by.feature.adjusted <- SCRIData.p.values.nonproliferating.by.feature;
SCRIData.p.values.nonproliferating.by.feature.adjusted[ primaryFeatures.Tenofovir ] <- p.adjust( SCRIData.p.values.nonproliferating.by.feature[ primaryFeatures.Tenofovir ], method = "holm" );
SCRIData.p.values.nonproliferating.by.feature.adjusted[ secondaryFeatures.Tenofovir ] <- p.adjust( SCRIData.p.values.nonproliferating.by.feature[ secondaryFeatures.Tenofovir ], method = "holm" );
SCRIData.p.values.nonproliferating.by.feature.adjusted[ secondaryFeatures.cancer10 ] <- p.adjust( SCRIData.p.values.nonproliferating.by.feature[ secondaryFeatures.cancer10 ], method = "holm" );
SCRIData.p.values.nonproliferating.by.feature.adjusted[ individualGOFeatures.Hallmark40 ] <- p.adjust( SCRIData.p.values.nonproliferating.by.feature[ individualGOFeatures.Hallmark40 ], method = "holm" );
SCRIData.p.values.nonproliferating.by.feature.adjusted[ individualGOFeatures.top20 ] <- p.adjust( SCRIData.p.values.nonproliferating.by.feature[ individualGOFeatures.top20 ], method = "holm" );

SCRI.uniqueIS.P.VALUES.ARE.COMPUTED.ALREADY <- TRUE;
} # End if SCRI.uniqueIS.P.VALUES.ARE.COMPUTED.ALREADY .. else ..
@ 

