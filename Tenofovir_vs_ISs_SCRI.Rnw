if( exists( "SCRI.genesWithAnyISs.SAMPLES.ARE.LOADED.ALREADY" ) && SCRI.genesWithAnyISs.SAMPLES.ARE.LOADED.ALREADY ) {
    ## GREAT.
} else {
### genesWithAnyISs analyses.
if( file.exists( SCRIData.genesWithAnyISs.the.samples.Rda.filename ) ) {
    load( file = SCRIData.genesWithAnyISs.the.samples.Rda.filename );
} else {
    set.seed( the.seed );
    WangData.num.genesWithAnyISs <- length( WangData.num.uniqueIS.by.gene );
    SCRIData.genesWithAnyISs.the.samples <- lapply( SCRIData.num.genesWithAnyISs.by.subject.and.total.and.totalProliferating, function ( .num.integration.sites ) {
        .rv <- t( rmultinom( num.samples, .num.integration.sites, rep( 1, WangData.num.genesWithAnyISs ) ) )
        colnames( .rv ) <- names( WangData.num.uniqueIS.by.gene );
        return( .rv );
    } );
    names( SCRIData.genesWithAnyISs.the.samples ) <- names( SCRIData.num.genesWithAnyISs.by.subject.and.total.and.totalProliferating );
    save( SCRIData.genesWithAnyISs.the.samples, file = SCRIData.genesWithAnyISs.the.samples.Rda.filename );
}

### Analysis based on the samples
if( file.exists( SCRIData.genesWithAnyISs.feature.nulls.Rda.filename ) ) {
    load( file = SCRIData.genesWithAnyISs.feature.nulls.Rda.filename );
} else {
    SCRIData.genesWithAnyISs.feature.nulls <- lapply( SCRIData.genesWithAnyISs.the.samples, function ( .the.samples ) {
        sapply( colnames( Wang.feature.by.gene ), function ( .feature ) {
            print( .feature );
            .feature.genes <- names( which( Wang.feature.by.gene[ , .feature ] ) );
            if( length( .feature.genes ) == 0 ) {
                return( rep( 0, nrow( .the.samples ) ) );
            } else {
                return( apply( .the.samples[ , .feature.genes, drop = FALSE ], 1, sum ) );
            }
        } )
    } );
    save( SCRIData.genesWithAnyISs.feature.nulls, file = SCRIData.genesWithAnyISs.feature.nulls.Rda.filename );
}

SCRI.genesWithAnyISs.SAMPLES.ARE.LOADED.ALREADY <- TRUE;
} # End if SCRI.genesWithAnyISs.SAMPLES.ARE.LOADED.ALREADY .. else ..@ 
